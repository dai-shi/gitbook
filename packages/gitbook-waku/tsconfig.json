{
    "compilerOptions": {
        "strict": true,
        "target": "esnext",
        "downlevelIteration": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "types": ["react/experimental"],
        "jsx": "react-jsx",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
